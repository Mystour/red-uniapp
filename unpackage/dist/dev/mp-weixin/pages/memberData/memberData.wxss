
.underline-text.data-v-6bfe28c6 {
	position: relative;
	line-height: 60rpx;
	margin: 20rpx 10rpx 10rpx 10rpx;
	display: inline-block;
	/* 确保块级元素也能应用下划线 */
}

/* 使用伪元素添加下划线 */
.underline-text.data-v-6bfe28c6::after {
	content: '';
	/* 伪元素必须有内容 */
	position: absolute;
	bottom: 0;
	/* 下划线位于文字底部 */
	left: 0;
	width: 100%;
	/* 横跨整个文本 */
	height: 13rpx;
	/* 下划线厚度 */
	opacity: 25%;
	padding: 10rpx 0rpx 5rpx 0rpx;
	margin-left: 30rpx;
	width: 150rpx;
	background-color: red;
	/* 下划线颜色 */
}

